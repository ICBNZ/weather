{
  "name": "express-boilerplate",
  "private": true,
  "scripts": {
    "start": "node ./node-app/bin/www",
    "server": "nodemon ./node-app/bin/www",
    "client": "cd weatherapp && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "express": "^4.17.1",
    "http-errors": "~1.7.2",
    "morgan": "~1.9.1",
    "nodemon": "^1.18.11",
    "pg-promise": "^8.5.6",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
